{
  "name": "angularspa",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "angular",
    "single page application",
    "oauth",
    "angular-cli",
    "typescript"
  ],
  "repository": "git+https://github.com/asadsahi/angularspa.git",
  "author": "Asad Sahi <asadazmat@gmail.com>",
  "scripts": {
    "ng": "ng",
    "clean:install": "rimraf node_modules & npm i",
    "start": "ng serve",
    "start:gh": "ng serve --baseHref /angularspa/ --deployUrl /angularspa/",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "swtest": "npx http-server dist -p 4200"
  },
  "dependencies": {
    "@angular/animations": "10.0.6",
    "@angular/common": "10.0.6",
    "@angular/compiler": "10.0.6",
    "@angular/compiler-cli": "10.0.6",
    "@angular/core": "11.0.5",
    "@angular/fire": "5.2.3",
    "@angular/forms": "10.0.6",
    "@angular/localize": "^10.0.6",
    "@angular/platform-browser": "10.0.6",
    "@angular/platform-browser-dynamic": "10.0.6",
    "@angular/platform-server": "10.0.6",
    "@angular/router": "10.0.6",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@microsoft/signalr": "3.1.6",
    "@microsoft/signalr-protocol-msgpack": "3.1.6",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "ag-grid-angular": "^23.2.1",
    "ag-grid-community": "^23.2.1",
    "bootstrap": "4.5.0",
    "date-fns": "2.15.0",
    "firebase": "7.17.1",
    "lodash": "4.17.19",
    "oidc-client": "1.10.1",
    "preboot": "7.0.0",
    "rxjs": "6.6.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "10.0.4",
    "@angular/language-service": "10.0.6",
    "@angular/pwa": "0.803.21",
    "@angular/service-worker": "10.0.6",
    "@compodoc/compodoc": "1.1.11",
    "@types/jest": "^26.0.7",
    "@types/node": "14.0.27",
    "codelyzer": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:check lint"
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/__mocks__/image.js",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "assets/(.*)": "<rootDir>/src/assets/$1",
      "environments/(.*)": "<rootDir>/src/environments/$1"
    }
  },
  "license": "MIT"
}
