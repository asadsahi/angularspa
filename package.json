{
  "name": "angularspa",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "angular",
    "single page application",
    "oauth",
    "angular-cli",
    "typescript"
  ],
  "repository": "git+https://github.com/asadsahi/angularspa.git",
  "author": "Asad Sahi <asadazmat@gmail.com>",
  "scripts": {
    "ng": "ng",
    "clean:install": "rimraf node_modules & npm i",
    "start": "ng serve",
    "start:gh": "ng serve --baseHref /angularspa/ --deployUrl /angularspa/",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "swtest": "npx http-server dist -p 4200"
  },
  "dependencies": {
    "@angular/animations": "9.1.12",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/compiler-cli": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/fire": "5.2.3",
    "@angular/forms": "9.1.12",
    "@angular/localize": "^9.1.12",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@angular/platform-server": "9.1.12",
    "@angular/router": "9.1.12",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@microsoft/signalr": "3.1.0",
    "@microsoft/signalr-protocol-msgpack": "3.1.0",
    "@ng-bootstrap/ng-bootstrap": "5.1.5",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "ag-grid-angular": "^22.1.1",
    "ag-grid-community": "^22.1.1",
    "bootstrap": "4.4.1",
    "date-fns": "2.8.1",
    "firebase": "7.6.1",
    "lodash": "4.17.15",
    "oidc-client": "1.10.1",
    "preboot": "7.0.0",
    "rxjs": "6.5.4",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "9.1.12",
    "@angular/language-service": "9.1.12",
    "@angular/pwa": "0.803.21",
    "@angular/service-worker": "9.1.12",
    "@compodoc/compodoc": "1.1.11",
    "@types/jest": "^24.0.25",
    "@types/node": "13.1.4",
    "codelyzer": "^5.2.1",
    "husky": "^4.0.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "5.4.2",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:check lint"
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/__mocks__/image.js",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "assets/(.*)": "<rootDir>/src/assets/$1",
      "environments/(.*)": "<rootDir>/src/environments/$1"
    }
  },
  "license": "MIT"
}
